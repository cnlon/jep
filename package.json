{
  "name": "jep",
  "version": "3.0.0",
  "description": "Jep (Javascript Expression Parser) is tiny library used to evaluate expression to javascript function",
  "main": "jep.js",
  "scripts": {
    "pretest": "npm run uglify",
    "test": "npm run test:normal && npm run test:uglified",
    "test:normal": "$(npm bin)/cross-env FILE=./jep.js $(npm bin)/mocha jep.test.js",
    "test:uglified": "$(npm bin)/cross-env FILE=./jep.min.js $(npm bin)/mocha jep.test.js",
    "uglify": "$(npm bin)/uglifyjs jep.js -d PRODUCTION=true -c -m -r 'Jep' --mangle-props --mangle-regex '/^_/' --comments '/jep/' -o jep.min.js --source-map jep.min.js.map"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnlon/jep.git"
  },
  "keywords": [
    "eval",
    "evaluate",
    "expression",
    "parse",
    "parser"
  ],
  "author": "lon <longhaohe@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cnlon/jep/issues"
  },
  "homepage": "https://github.com/cnlon/jep#readme",
  "devDependencies": {
    "chai": "^3.5.0",
    "cross-env": "^3.1.3",
    "mocha": "^3.1.2",
    "uglify-js": "^2.7.4"
  }
}
